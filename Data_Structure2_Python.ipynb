    "5. Write a code to find all occurrences of a given substring within another string.\n",
    "Ans.\n",
    "    # Function to find all occurrences of a substring\n",
    "def find_all_occurrences(main_string, sub_string):\n",
    "    start = 0\n",
    "    positions = []\n",
    "\n",
    "    # Loop to find all occurrences of sub_string in main_string\n",
    "    while True:\n",
    "        # Use the find() method to locate the substring\n",
    "        start = main_string.find(sub_string, start)\n",
    "\n",
    "        # If find() returns -1, no more occurrences are found\n",
    "        if start == -1:\n",
    "            break\n",
    "\n",
    "        # Append the position of the found substring\n",
    "        positions.append(start)\n",
    "\n",
    "        # Move the starting index forward for the next search\n",
    "        start += len(sub_string)\n",
    "\n",
    "    return positions\n",
    "\n",
    "    # Example usage\n",
    "    main_string = \"This is a test string. Let's test this.\"\n",
    "    sub_string = \"test\"\n",
    "\n",
    "    positions = find_all_occurrences(main_string, sub_string)\n",
    "\n",
    "    if positions:\n",
    "        print(f\"Substring '{sub_string}' found at positions: {positions}\")\n",
    "    else:\n",
    "        print(f\"Substring '{sub_string}' not found.\")\n",
    "\n",
    "  #Output\n",
    "  \n",
    "    Substring 'test' found at positions: [10, 28]\n"
   ]
